{% extends 'base.html.twig' %}

{% block title %}User form{% endblock %}

{% block body %}
    <h1>{% block pagetitle %}User form{% endblock %}</h1>
    {{form_start(form)}}
        {{form_row(form.email)}}
        {{form_row(form.birthdate)}}
        {{form_row(form.submit)}}        
        {{form_rest(form)}}
    {{form_end(form, {'render_rest': false})}}
{% endblock %}
